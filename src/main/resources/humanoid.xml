<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>
    <bean id="vova" class="com.by.petranovski.myspring.scanbean.bean.Human">
        <property name="name" value="Vavan"/>
        <property name="weight" value="75"/>
    </bean>
    <bean id="head" class="com.by.petranovski.myspring.scanbean.bean.Head">
        <property name="years" value="5"/>
    </bean>
    <bean id="head2" class="com.by.petranovski.myspring.scanbean.bean.Head">
<!--        <property name="years" value="2"/>-->
    </bean>
</beans>